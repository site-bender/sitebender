@prefix : <http://sitebender.io/ontology/country#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix schema: <http://schema.org/> .
@prefix wdt: <http://www.wikidata.org/prop/direct/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

################################################################################
# ONTOLOGY METADATA
################################################################################

<http://sitebender.io/ontology/country> a owl:Ontology ;
	owl:versionIRI <http://sitebender.io/ontology/country/1.0.0> ;
	owl:versionInfo "1.0.0" ;
	dct:title "Country Ontology"@en ;
	dct:description "Comprehensive OWL2 ontology for representing countries, territories, and their properties including ISO codes, time zones, geographic data, demographics, and political information."@en ;
	dct:creator "Sitebender" ;
	dct:created "2025-01-13"^^xsd:date ;
	dct:modified "2025-01-13"^^xsd:date ;
	dct:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
	rdfs:comment "This ontology provides a comprehensive model for country data with OWL2 semantics and SHACL validation. It aligns with existing vocabularies (DBpedia, Schema.org, Wikidata) and uses GeoSPARQL for geographic properties."@en .

################################################################################
# CLASSES
################################################################################

## Core Geographic Entities

:Country a owl:Class ;
	rdfs:label "Country"@en ;
	rdfs:comment "A geopolitical entity recognized as a distinct territorial unit. This includes sovereign states, dependent territories, and disputed areas."@en ;
	owl:equivalentClass dbo:Country , schema:Country ;
	skos:prefLabel "Country"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:SovereignState a owl:Class ;
	rdfs:label "Sovereign State"@en ;
	rdfs:label "État souverain"@fr ;
	rdfs:label "Estado soberano"@es ;
	rdfs:comment "An independent country with full sovereignty over its territory and population."@en ;
	rdfs:subClassOf :Country ;
	owl:equivalentClass dbo:Country ;
	skos:prefLabel "Sovereign State"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:Territory a owl:Class ;
	rdfs:label "Dependent Territory"@en ;
	rdfs:label "Territoire dépendant"@fr ;
	rdfs:label "Territorio dependiente"@es ;
	rdfs:comment "A geographic area under the jurisdiction of another country, without full sovereignty."@en ;
	rdfs:subClassOf :Country ;
	skos:prefLabel "Dependent Territory"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:DisputedTerritory a owl:Class ;
	rdfs:label "Disputed Territory"@en ;
	rdfs:label "Territoire contesté"@fr ;
	rdfs:label "Territorio disputado"@es ;
	rdfs:comment "A geographic area whose sovereignty is contested by two or more countries."@en ;
	rdfs:subClassOf :Country ;
	skos:prefLabel "Disputed Territory"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Administrative and Political Entities

:Capital a owl:Class ;
	rdfs:label "Capital City"@en ;
	rdfs:label "Capitale"@fr ;
	rdfs:label "Capital"@es ;
	rdfs:comment "The primary city serving as the seat of government for a country."@en ;
	owl:equivalentClass dbo:Capital , schema:City ;
	skos:prefLabel "Capital City"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:GovernmentType a owl:Class ;
	rdfs:label "Government Type"@en ;
	rdfs:label "Type de gouvernement"@fr ;
	rdfs:label "Tipo de gobierno"@es ;
	rdfs:comment "The system or form of governance used by a country (e.g., republic, monarchy, federation)."@en ;
	skos:prefLabel "Government Type"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:RegionalOrganization a owl:Class ;
	rdfs:label "Regional Organization"@en ;
	rdfs:label "Organisation régionale"@fr ;
	rdfs:label "Organización regional"@es ;
	rdfs:comment "An intergovernmental organization whose members are countries from a specific geographic region."@en ;
	owl:equivalentClass dbo:Organisation ;
	skos:prefLabel "Regional Organization"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Time and Temporal Entities

:TimeZone a owl:Class ;
	rdfs:label "Time Zone"@en ;
	rdfs:label "Fuseau horaire"@fr ;
	rdfs:label "Zona horaria"@es ;
	rdfs:comment "A region that observes a uniform standard time for legal, commercial, and social purposes."@en ;
	rdfs:subClassOf time:TemporalEntity ;
	skos:prefLabel "Time Zone"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Economic Entities

:Currency a owl:Class ;
	rdfs:label "Currency"@en ;
	rdfs:label "Monnaie"@fr ;
	rdfs:label "Moneda"@es ;
	rdfs:comment "A system of money in general use in a particular country."@en ;
	owl:equivalentClass dbo:Currency , schema:Currency ;
	skos:prefLabel "Currency"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Linguistic Entities

:Language a owl:Class ;
	rdfs:label "Language"@en ;
	rdfs:label "Langue"@fr ;
	rdfs:label "Idioma"@es ;
	rdfs:comment "A natural language used for communication within a country."@en ;
	owl:equivalentClass dbo:Language , schema:Language ;
	skos:prefLabel "Language"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Demographic Entities

:PopulationCount a owl:Class ;
	rdfs:label "Population Count"@en ;
	rdfs:label "Décompte de population"@fr ;
	rdfs:label "Recuento de población"@es ;
	rdfs:comment "A measurement of the number of people in a country at a specific point in time."@en ;
	rdfs:subClassOf time:TemporalEntity ;
	skos:prefLabel "Population Count"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Class Disjointness

:SovereignState owl:disjointWith :Territory , :DisputedTerritory .
:Territory owl:disjointWith :DisputedTerritory .

################################################################################
# OBJECT PROPERTIES - Geographic and Political Relationships
################################################################################

:capital a owl:ObjectProperty , owl:FunctionalProperty ;
	rdfs:label "capital"@en ;
	rdfs:label "capitale"@fr ;
	rdfs:label "capital"@es ;
	rdfs:comment "The capital city of a country."@en ;
	rdfs:domain :Country ;
	rdfs:range :Capital ;
	owl:equivalentProperty dbo:capital , schema:capital ;
	skos:prefLabel "capital"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:bordersWith a owl:ObjectProperty , owl:SymmetricProperty ;
	rdfs:label "borders with"@en ;
	rdfs:label "frontière avec"@fr ;
	rdfs:label "limita con"@es ;
	rdfs:comment "Indicates that two countries share a land or maritime border."@en ;
	rdfs:domain :Country ;
	rdfs:range :Country ;
	owl:equivalentProperty dbo:border ;
	skos:prefLabel "borders with"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:dependencyOf a owl:ObjectProperty ;
	rdfs:label "dependency of"@en ;
	rdfs:label "dépendance de"@fr ;
	rdfs:label "dependencia de"@es ;
	rdfs:comment "Indicates that a territory is a dependency of another country."@en ;
	rdfs:domain :Territory ;
	rdfs:range :SovereignState ;
	owl:inverseOf :hasDependency ;
	skos:prefLabel "dependency of"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:hasDependency a owl:ObjectProperty ;
	rdfs:label "has dependency"@en ;
	rdfs:label "a une dépendance"@fr ;
	rdfs:label "tiene dependencia"@es ;
	rdfs:comment "Indicates that a country has a dependent territory."@en ;
	rdfs:domain :SovereignState ;
	rdfs:range :Territory ;
	owl:inverseOf :dependencyOf ;
	skos:prefLabel "has dependency"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:memberOf a owl:ObjectProperty ;
	rdfs:label "member of"@en ;
	rdfs:label "membre de"@fr ;
	rdfs:label "miembro de"@es ;
	rdfs:comment "Indicates that a country is a member of a regional organization."@en ;
	rdfs:domain :Country ;
	rdfs:range :RegionalOrganization ;
	owl:inverseOf :hasMember ;
	skos:prefLabel "member of"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:hasMember a owl:ObjectProperty ;
	rdfs:label "has member"@en ;
	rdfs:label "a pour membre"@fr ;
	rdfs:label "tiene miembro"@es ;
	rdfs:comment "Indicates that a regional organization has a country as a member."@en ;
	rdfs:domain :RegionalOrganization ;
	rdfs:range :Country ;
	owl:inverseOf :memberOf ;
	skos:prefLabel "has member"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:governmentType a owl:ObjectProperty , owl:FunctionalProperty ;
	rdfs:label "government type"@en ;
	rdfs:label "type de gouvernement"@fr ;
	rdfs:label "tipo de gobierno"@es ;
	rdfs:comment "The form of government used by a country."@en ;
	rdfs:domain :Country ;
	rdfs:range :GovernmentType ;
	owl:equivalentProperty dbo:governmentType ;
	skos:prefLabel "government type"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Time Zone Relationships

:hasTimeZone a owl:ObjectProperty ;
	rdfs:label "has time zone"@en ;
	rdfs:label "a un fuseau horaire"@fr ;
	rdfs:label "tiene zona horaria"@es ;
	rdfs:comment "A time zone observed within a country's territory."@en ;
	rdfs:domain :Country ;
	rdfs:range :TimeZone ;
	skos:prefLabel "has time zone"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:timeZoneOf a owl:ObjectProperty ;
	rdfs:label "time zone of"@en ;
	rdfs:label "fuseau horaire de"@fr ;
	rdfs:label "zona horaria de"@es ;
	rdfs:comment "The country or countries that observe this time zone."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range :Country ;
	owl:inverseOf :hasTimeZone ;
	skos:prefLabel "time zone of"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Economic Relationships

:currency a owl:ObjectProperty ;
	rdfs:label "currency"@en ;
	rdfs:label "monnaie"@fr ;
	rdfs:label "moneda"@es ;
	rdfs:comment "An official currency used in a country."@en ;
	rdfs:domain :Country ;
	rdfs:range :Currency ;
	owl:equivalentProperty dbo:currency , schema:currency ;
	skos:prefLabel "currency"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Linguistic Relationships

:officialLanguage a owl:ObjectProperty ;
	rdfs:label "official language"@en ;
	rdfs:label "langue officielle"@fr ;
	rdfs:label "idioma oficial"@es ;
	rdfs:comment "A language that has official status in a country."@en ;
	rdfs:domain :Country ;
	rdfs:range :Language ;
	owl:equivalentProperty dbo:officialLanguage ;
	skos:prefLabel "official language"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:spokenLanguage a owl:ObjectProperty ;
	rdfs:label "spoken language"@en ;
	rdfs:label "langue parlée"@fr ;
	rdfs:label "idioma hablado"@es ;
	rdfs:comment "A language commonly spoken in a country, whether official or not."@en ;
	rdfs:domain :Country ;
	rdfs:range :Language ;
	skos:prefLabel "spoken language"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

## Demographic Relationships

:populationCount a owl:ObjectProperty ;
	rdfs:label "population count"@en ;
	rdfs:label "décompte de population"@fr ;
	rdfs:label "recuento de población"@es ;
	rdfs:comment "A temporal population measurement for a country."@en ;
	rdfs:domain :Country ;
	rdfs:range :PopulationCount ;
	skos:prefLabel "population count"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

################################################################################
# DATATYPE PROPERTIES - Identity Codes
################################################################################

:alpha2Code a owl:DatatypeProperty , owl:FunctionalProperty , owl:InverseFunctionalProperty ;
	rdfs:label "ISO 3166-1 alpha-2 code"@en ;
	rdfs:label "code alpha-2 ISO 3166-1"@fr ;
	rdfs:label "código alfa-2 ISO 3166-1"@es ;
	rdfs:comment "Two-letter country code defined in ISO 3166-1 alpha-2 standard."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:string ;
	owl:equivalentProperty wdt:P297 ;
	skos:prefLabel "alpha-2 code"@en ;
	skos:example "US", "FR", "JP" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:alpha3Code a owl:DatatypeProperty , owl:FunctionalProperty , owl:InverseFunctionalProperty ;
	rdfs:label "ISO 3166-1 alpha-3 code"@en ;
	rdfs:label "code alpha-3 ISO 3166-1"@fr ;
	rdfs:label "código alfa-3 ISO 3166-1"@es ;
	rdfs:comment "Three-letter country code defined in ISO 3166-1 alpha-3 standard."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:string ;
	owl:equivalentProperty wdt:P298 ;
	skos:prefLabel "alpha-3 code"@en ;
	skos:example "USA", "FRA", "JPN" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:numericCode a owl:DatatypeProperty , owl:FunctionalProperty , owl:InverseFunctionalProperty ;
	rdfs:label "ISO 3166-1 numeric code"@en ;
	rdfs:label "code numérique ISO 3166-1"@fr ;
	rdfs:label "código numérico ISO 3166-1"@es ;
	rdfs:comment "Three-digit country code defined in ISO 3166-1 numeric standard."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:string ;
	owl:equivalentProperty wdt:P299 ;
	skos:prefLabel "numeric code"@en ;
	skos:example "840", "250", "392" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

################################################################################
# DATATYPE PROPERTIES - Telephone and Communication
################################################################################

:callingCode a owl:DatatypeProperty ;
	rdfs:label "international calling code"@en ;
	rdfs:label "indicatif téléphonique international"@fr ;
	rdfs:label "código de llamada internacional"@es ;
	rdfs:comment "International telephone dialing code for a country, including the '+' prefix."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:string ;
	owl:equivalentProperty dbo:callingCode , wdt:P474 ;
	skos:prefLabel "calling code"@en ;
	skos:example "+1", "+33", "+81" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:countryCallingCodePrefix a owl:DatatypeProperty ;
	rdfs:label "country calling code prefix"@en ;
	rdfs:label "préfixe d'indicatif de pays"@fr ;
	rdfs:label "prefijo de código de país"@es ;
	rdfs:comment "The numeric portion of the calling code without the '+' symbol."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:positiveInteger ;
	skos:prefLabel "calling code prefix"@en ;
	skos:example "1", "33", "81" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:internetTLD a owl:DatatypeProperty ;
	rdfs:label "internet top-level domain"@en ;
	rdfs:label "domaine de premier niveau internet"@fr ;
	rdfs:label "dominio de nivel superior de internet"@es ;
	rdfs:comment "Country code top-level domain (ccTLD) for a country."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:string ;
	owl:equivalentProperty wdt:P78 ;
	skos:prefLabel "internet TLD"@en ;
	skos:example ".us", ".fr", ".jp" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

################################################################################
# DATATYPE PROPERTIES - Time Zone Details
################################################################################

:ianaIdentifier a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "IANA time zone identifier"@en ;
	rdfs:label "identifiant de fuseau horaire IANA"@fr ;
	rdfs:label "identificador de zona horaria IANA"@es ;
	rdfs:comment "The standardized identifier from the IANA Time Zone Database."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range xsd:string ;
	skos:prefLabel "IANA identifier"@en ;
	skos:example "America/New_York", "Europe/Paris", "Asia/Tokyo" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:utcOffset a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "UTC offset"@en ;
	rdfs:label "décalage UTC"@fr ;
	rdfs:label "compensación UTC"@es ;
	rdfs:comment "The standard time offset from Coordinated Universal Time (UTC)."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range xsd:dayTimeDuration ;
	skos:prefLabel "UTC offset"@en ;
	skos:example "-PT5H", "PT1H", "PT9H" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:utcOffsetDST a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "UTC offset during DST"@en ;
	rdfs:label "décalage UTC en heure d'été"@fr ;
	rdfs:label "compensación UTC durante horario de verano"@es ;
	rdfs:comment "The time offset from UTC when daylight saving time is in effect."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range xsd:dayTimeDuration ;
	skos:prefLabel "UTC offset DST"@en ;
	skos:example "-PT4H", "PT2H", "PT9H" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:observesDST a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "observes daylight saving time"@en ;
	rdfs:label "observe l'heure d'été"@fr ;
	rdfs:label "observa horario de verano"@es ;
	rdfs:comment "Indicates whether the time zone observes daylight saving time."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range xsd:boolean ;
	skos:prefLabel "observes DST"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:timeZoneName a owl:DatatypeProperty ;
	rdfs:label "time zone name"@en ;
	rdfs:label "nom du fuseau horaire"@fr ;
	rdfs:label "nombre de zona horaria"@es ;
	rdfs:comment "A human-readable name for the time zone."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range rdf:langString ;
	skos:prefLabel "time zone name"@en ;
	skos:example "Eastern Standard Time"@en, "Heure normale de l'Est"@fr ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:timeZoneAbbreviation a owl:DatatypeProperty ;
	rdfs:label "time zone abbreviation"@en ;
	rdfs:label "abréviation du fuseau horaire"@fr ;
	rdfs:label "abreviatura de zona horaria"@es ;
	rdfs:comment "Standard abbreviation for the time zone."@en ;
	rdfs:domain :TimeZone ;
	rdfs:range xsd:string ;
	skos:prefLabel "time zone abbreviation"@en ;
	skos:example "EST", "CET", "JST" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

################################################################################
# DATATYPE PROPERTIES - Geographic and Spatial
################################################################################

:latitude a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "latitude"@en ;
	rdfs:label "latitude"@fr ;
	rdfs:label "latitud"@es ;
	rdfs:comment "The geographic coordinate that specifies the north-south position of the country's center."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:decimal ;
	owl:equivalentProperty geo:lat , schema:latitude ;
	skos:prefLabel "latitude"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:longitude a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "longitude"@en ;
	rdfs:label "longitude"@fr ;
	rdfs:label "longitud"@es ;
	rdfs:comment "The geographic coordinate that specifies the east-west position of the country's center."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:decimal ;
	owl:equivalentProperty geo:long , schema:longitude ;
	skos:prefLabel "longitude"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:geometry a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "geometry"@en ;
	rdfs:label "géométrie"@fr ;
	rdfs:label "geometría"@es ;
	rdfs:comment "The GeoSPARQL geometry representing the country's borders and territory."@en ;
	rdfs:domain :Country ;
	rdfs:range geo:wktLiteral ;
	owl:equivalentProperty geo:asWKT ;
	skos:prefLabel "geometry"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:area a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "area"@en ;
	rdfs:label "superficie"@fr ;
	rdfs:label "área"@es ;
	rdfs:comment "The total area of the country in square kilometers."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:decimal ;
	owl:equivalentProperty dbo:areaTotal , schema:areaServed ;
	skos:prefLabel "area"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:landArea a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "land area"@en ;
	rdfs:label "superficie terrestre"@fr ;
	rdfs:label "área terrestre"@es ;
	rdfs:comment "The land area of the country in square kilometers, excluding water bodies."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:decimal ;
	skos:prefLabel "land area"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:waterArea a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "water area"@en ;
	rdfs:label "superficie d'eau"@fr ;
	rdfs:label "área acuática"@es ;
	rdfs:comment "The water area of the country in square kilometers."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:decimal ;
	skos:prefLabel "water area"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:coastlineLength a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:label "coastline length"@en ;
	rdfs:label "longueur du littoral"@fr ;
	rdfs:label "longitud de costa"@es ;
	rdfs:comment "The length of the country's coastline in kilometers."@en ;
	rdfs:domain :Country ;
	rdfs:range xsd:decimal ;
	skos:prefLabel "coastline length"@en ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

################################################################################
# DATATYPE PROPERTIES - Economic
################################################################################

:currencyCode a owl:DatatypeProperty , owl:InverseFunctionalProperty ;
	rdfs:label "ISO 4217 currency code"@en ;
	rdfs:label "code de devise ISO 4217"@fr ;
	rdfs:label "código de moneda ISO 4217"@es ;
	rdfs:comment "The three-letter currency code defined in ISO 4217 standard."@en ;
	rdfs:domain :Currency ;
	rdfs:range xsd:string ;
	owl:equivalentProperty wdt:P498 ;
	skos:prefLabel "currency code"@en ;
	skos:example "USD", "EUR", "JPY" ;
	rdfs:isDefinedBy <http://sitebender.io/ontology/country> .

:currencyNumericCode a owl:DatatypeProperty , owl:FunctionalProperty , owl:InverseFunctionalProperty ;
	rdfs:label "ISO 4217 numeric currency code"@en ;
