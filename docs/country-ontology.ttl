################################################################################
# SHACL SHAPES - Validation Constraints
################################################################################

## Country Shape

:CountryShape a sh:NodeShape ;
	sh:targetClass :Country ;
	sh:name "Country Shape"@en ;
	sh:description "Validation constraints for Country instances."@en ;

	# Required properties with cardinality
	sh:property [
		sh:path rdfs:label ;
		sh:minCount 1 ;
		sh:datatype rdf:langString ;
		sh:uniqueLang true ;
		sh:name "country name"@en ;
		sh:description "Every country must have at least one language-tagged label."@en ;
	] ;

	sh:property [
		sh:path :alpha2Code ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[A-Z]{2}$" ;
		sh:name "ISO 3166-1 alpha-2"@en ;
		sh:description "Must be exactly 2 uppercase letters."@en ;
		sh:message "Alpha-2 code must be exactly two uppercase letters (e.g., 'US', 'FR')."@en ;
	] ;

	sh:property [
		sh:path :alpha3Code ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[A-Z]{3}$" ;
		sh:name "ISO 3166-1 alpha-3"@en ;
		sh:description "Must be exactly 3 uppercase letters."@en ;
		sh:message "Alpha-3 code must be exactly three uppercase letters (e.g., 'USA', 'FRA')."@en ;
	] ;

	sh:property [
		sh:path :numericCode ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[0-9]{3}$" ;
		sh:name "ISO 3166-1 numeric"@en ;
		sh:description "Must be exactly 3 digits."@en ;
		sh:message "Numeric code must be exactly three digits (e.g., '840', '250')."@en ;
	] ;

	# Optional properties with constraints
	sh:property [
		sh:path :callingCode ;
		sh:datatype xsd:string ;
		sh:pattern "^\\+[0-9]{1,4}$" ;
		sh:name "calling code"@en ;
		sh:message "Calling code must start with '+' followed by 1-4 digits."@en ;
	] ;

	sh:property [
		sh:path :internetTLD ;
		sh:datatype xsd:string ;
		sh:pattern "^\\.[a-z]{2,}$" ;
		sh:name "internet TLD"@en ;
		sh:message "Internet TLD must start with '.' followed by lowercase letters."@en ;
	] ;

	sh:property [
		sh:path :latitude ;
		sh:maxCount 1 ;
		sh:datatype xsd:decimal ;
		sh:minInclusive -90.0 ;
		sh:maxInclusive 90.0 ;
		sh:name "latitude"@en ;
		sh:message "Latitude must be between -90 and 90 degrees."@en ;
	] ;

	sh:property [
		sh:path :longitude ;
		sh:maxCount 1 ;
		sh:datatype xsd:decimal ;
		sh:minInclusive -180.0 ;
		sh:maxInclusive 180.0 ;
		sh:name "longitude"@en ;
		sh:message "Longitude must be between -180 and 180 degrees."@en ;
	] ;

	sh:property [
		sh:path :area ;
		sh:maxCount 1 ;
		sh:datatype xsd:decimal ;
		sh:minExclusive 0.0 ;
		sh:name "area"@en ;
		sh:message "Area must be a positive number."@en ;
	] ;

	sh:property [
		sh:path :population ;
		sh:datatype xsd:nonNegativeInteger ;
		sh:minInclusive 0 ;
		sh:name "population"@en ;
	] ;

	sh:property [
		sh:path :capital ;
		sh:maxCount 1 ;
		sh:class :Capital ;
		sh:name "capital"@en ;
	] .

## Time Zone Shape

:TimeZoneShape a sh:NodeShape ;
	sh:targetClass :TimeZone ;
	sh:name "Time Zone Shape"@en ;
	sh:description "Validation constraints for TimeZone instances."@en ;

	sh:property [
		sh:path :ianaIdentifier ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[A-Za-z_]+/[A-Za-z_]+$" ;
		sh:name "IANA identifier"@en ;
		sh:message "IANA identifier must be in format 'Region/City' (e.g., 'America/New_York')."@en ;
	] ;

	sh:property [
		sh:path :utcOffset ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:dayTimeDuration ;
		sh:name "UTC offset"@en ;
	] ;

	sh:property [
		sh:path :observesDST ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:boolean ;
		sh:name "observes DST"@en ;
	] ;

	# If observesDST is true, utcOffsetDST must be present
	sh:sparql [
		sh:message "Time zones that observe DST must have a utcOffsetDST property."@en ;
		sh:prefixes : ;
		sh:select """
			SELECT $this
			WHERE {
				$this :observesDST true .
				FILTER NOT EXISTS { $this :utcOffsetDST ?offset }
			}
		""" ;
	] .

## Currency Shape

:CurrencyShape a sh:NodeShape ;
	sh:targetClass :Currency ;
	sh:name "Currency Shape"@en ;
	sh:description "Validation constraints for Currency instances."@en ;

	sh:property [
		sh:path :currencyCode ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[A-Z]{3}$" ;
		sh:name "ISO 4217 code"@en ;
		sh:message "Currency code must be exactly three uppercase letters (e.g., 'USD', 'EUR')."@en ;
	] ;

	sh:property [
		sh:path :currencyNumericCode ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[0-9]{3}$" ;
		sh:name "ISO 4217 numeric code"@en ;
		sh:message "Currency numeric code must be exactly three digits."@en ;
	] ;

	sh:property [
		sh:path rdfs:label ;
		sh:minCount 1 ;
		sh:datatype rdf:langString ;
		sh:uniqueLang true ;
		sh:name "currency name"@en ;
	] .

## Language Shape

:LanguageShape a sh:NodeShape ;
	sh:targetClass :Language ;
	sh:name "Language Shape"@en ;
	sh:description "Validation constraints for Language instances."@en ;

	sh:property [
		sh:path :iso6391Code ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[a-z]{2}$" ;
		sh:name "ISO 639-1 code"@en ;
		sh:message "ISO 639-1 code must be exactly two lowercase letters."@en ;
	] ;

	sh:property [
		sh:path :iso6392Code ;
		sh:maxCount 1 ;
		sh:datatype xsd:string ;
		sh:pattern "^[a-z]{3}$" ;
		sh:name "ISO 639-2 code"@en ;
		sh:message "ISO 639-2 code must be exactly three lowercase letters."@en ;
	] ;

	sh:property [
		sh:path rdfs:label ;
		sh:minCount 1 ;
		sh:datatype rdf:langString ;
		sh:uniqueLang true ;
		sh:name "language name"@en ;
	] .

## Population Count Shape

:PopulationCountShape a sh:NodeShape ;
	sh:targetClass :PopulationCount ;
	sh:name "Population Count Shape"@en ;
	sh:description "Validation constraints for PopulationCount instances."@en ;

	sh:property [
		sh:path :populationValue ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:nonNegativeInteger ;
		sh:name "population value"@en ;
	] ;

	sh:property [
		sh:path :populationDate ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:datatype xsd:date ;
		sh:name "population date"@en ;
	] ;

	sh:property [
		sh:path :dataSource ;
		sh:minCount 1 ;
		sh:datatype xsd:anyURI ;
		sh:name "data source"@en ;
		sh:message "Population counts must cite their data source."@en ;
	] .

## Sovereign State Shape (extends Country Shape)

:SovereignStateShape a sh:NodeShape ;
	sh:targetClass :SovereignState ;
	sh:name "Sovereign State Shape"@en ;
	sh:description "Additional validation constraints specific to sovereign states."@en ;

	sh:property [
		sh:path :sovereigntyStatus ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:hasValue "independent" ;
		sh:name "sovereignty status"@en ;
		sh:message "Sovereign states must have sovereignty status 'independent'."@en ;
	] ;

	sh:property [
		sh:path :capital ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:class :Capital ;
		sh:name "capital"@en ;
		sh:message "Sovereign states must have exactly one capital city."@en ;
	] .

## Territory Shape (extends Country Shape)

:TerritoryShape a sh:NodeShape ;
	sh:targetClass :Territory ;
	sh:name "Territory Shape"@en ;
	sh:description "Additional validation constraints specific to dependent territories."@en ;

	sh:property [
		sh:path :dependencyOf ;
		sh:minCount 1 ;
		sh:maxCount 1 ;
		sh:class :SovereignState ;
		sh:name "dependency of"@en ;
		sh:message "Territories must be a dependency of exactly one sovereign state."@en ;
	] .

## Confidence Level Constraint

:ConfidenceLevelShape a sh:NodeShape ;
	sh:targetSubjectsOf :confidenceLevel ;
	sh:property [
		sh:path :confidenceLevel ;
		sh:datatype xsd:decimal ;
		sh:minInclusive 0.0 ;
		sh:maxInclusive 1.0 ;
		sh:name "confidence level"@en ;
		sh:message "Confidence level must be between 0.0 and 1.0."@en ;
	] .
